<!-- XAML version of the icon for use in WPF -->
<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Width="256" Height="256">
    
    <!-- Background circle -->
    <Ellipse Width="240" Height="240" Canvas.Left="8" Canvas.Top="8" 
             Fill="#2563eb" Stroke="#1e40af" StrokeThickness="4"/>
    
    <!-- Clipboard base -->
    <Rectangle Width="120" Height="160" Canvas.Left="68" Canvas.Top="58" 
               RadiusX="8" RadiusY="8" Fill="White" Stroke="#1e40af" StrokeThickness="3"/>
    
    <!-- Clipboard clip -->
    <Rectangle Width="60" Height="40" Canvas.Left="98" Canvas.Top="38" 
               RadiusX="6" RadiusY="6" Fill="#1e40af"/>
    <Rectangle Width="40" Height="20" Canvas.Left="108" Canvas.Top="48" 
               RadiusX="4" RadiusY="4" Fill="White"/>
    
    <!-- Document lines -->
    <Rectangle Width="80" Height="6" Canvas.Left="88" Canvas.Top="98" 
               RadiusX="2" RadiusY="2" Fill="#64748b"/>
    <Rectangle Width="60" Height="6" Canvas.Left="88" Canvas.Top="114" 
               RadiusX="2" RadiusY="2" Fill="#64748b"/>
    <Rectangle Width="70" Height="6" Canvas.Left="88" Canvas.Top="130" 
               RadiusX="2" RadiusY="2" Fill="#64748b"/>
    
    <!-- MD label -->
    <Border Width="80" Height="40" Canvas.Left="88" Canvas.Top="155" 
            CornerRadius="4" Background="#1e40af">
        <TextBlock Text="MD" FontSize="24" FontWeight="Bold" 
                   Foreground="White" HorizontalAlignment="Center" 
                   VerticalAlignment="Center"/>
    </Border>
    
    <!-- Arrow -->
    <Path Data="M 188 128 L 208 128 L 208 118 L 228 138 L 208 158 L 208 148 L 188 148 Z" 
          Fill="#10b981" Stroke="#059669" StrokeThickness="2"/>
</Canvas>